{
  "compilerOptions": {
    "target": "es2019",
    "allowJs": false,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "downlevelIteration": true
  },
  "include": ["."]
}
